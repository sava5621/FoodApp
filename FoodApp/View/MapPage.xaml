<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FoodApp.View.MapPage"
             xmlns:model="clr-namespace:FoodApp.Model.View"
             xmlns:viewmodel="clr-namespace:FoodApp.ViewModel"
             x:DataType="viewmodel:MapViewModel"
             xmlns:control="clr-namespace:FoodApp.Controlers;assembly=CustomRenderer"
             Title="MapPage">
    <VerticalStackLayout>
        <Frame>
            <Editor Text="{Binding Url}"/>
        </Frame>
        <Button></Button>
        <control:WebViewer  Source="{Binding Url}" 
                            EvaluateJavascript="{Binding EvaluateJavascript, Mode=OneWayToSource}"/>
    </VerticalStackLayout>
   
</ContentPage>
